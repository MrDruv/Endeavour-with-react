generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todo {
  id        Int       @id @default(autoincrement())
  text      String    @db.VarChar(255)
  completed Boolean?  @default(false)
  due_date  DateTime? @db.Date
  notes     String?
  createdat DateTime @default(now())
}
